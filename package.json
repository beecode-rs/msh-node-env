  "homepage": "https://github.com/beecode-rs/msh-node-env#readme",
  "keywords": [],
  "license": "mit",
  "main": "./lib/index.js",
  "name": "@beecode/msh-node-env",
  "repository": {
    "type": "git",
    "url": "https://github.com/beecode-rs/msh-node-env.git"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "json-schema": "^0.4.0",
    "y18n": "^5.0.8"
  },
  "scripts": {
    "audit:fix": "npx --yes npm-force-resolutions && sleep 1 && npm i",
    "build": "npm run clean && tsc -p ./tsconfig-build.json",
    "build:watch": "npm run clean && tsc-watch -p tsconfig-build.json --preserveWatchOutput",
    "bump-version": "rjp package.json version",
    "clean": "rimraf ./lib/*",
    "commit": "git-cz",
    "coverage": "nyc npm run test:unit",
    "coverage:publish": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "doc:toc": "markdown-toc -i ./README.md",
    "eslint": "eslint . --ext .ts --quiet",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --check \"./src/**/*.{ts,json}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{ts,json}\"",
    "semantic-release": "semantic-release",
    "semantic-release:check": "npm run semantic-release -- --dry-run --no-ci",
    "test": "npm run test:unit && npm run test:int",
    "test:int": "jest --config=./test/jest.config.json",
    "test:unit": "jest",
    "tsc": "tsc -p ./tsconfig-build.json",
    "tsc:check": "npm run tsc -- --noEmit",
    "tsc:cleaner": "ts-cleaner --dist lib",
    "tsc:cleaner:watch": "npm run tsc:cleaner -- --watch",
    "tsc:watch": "tsc-watch -p tsconfig-build.json"
  },
  "typings": "./lib/index.d.ts",
  "version": "0.2.4"
}
