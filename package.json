    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-no-only-tests": "2.6.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-extended": "1.1.0",
    "markdown-toc": "1.2.0",
    "nyc": "15.1.0",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "replace-json-property": "1.8.0",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.0",
    "source-map-support": "0.5.21",
