{
  "author": "Milos Bugarinovic <milos.bugarinovic@gmail.com>",
  "bugs": {
    "url": "https://github.com/beecode-rs/msh-node-env/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "base-64": "1.0.0"
  },
  "description": "",
  "devDependencies": {
    "@types/base-64": "0.1.3",
    "@types/chai": "4.2.14",
    "@types/mocha": "8.0.4",
    "@types/node": "14.14.10",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "9.0.9",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "chai": "4.2.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "mocha": "8.2.1",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "proxyquire": "2.1.3",
    "rimraf": "3.0.2",
    "sinon": "9.2.1",
    "ts-node": "9.0.0",
    "tsc-watch-2": "0.0.2",
    "typescript": "4.1.2"
  },
  "files": [
    "lib",
    "package-lock.json"
  ],
  "homepage": "https://github.com/beecode-rs/msh-node-env#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run test:lint"
    }
  },
  "keywords": [],
  "license": "mit",
  "main": "./lib/index.js",
  "name": "@beecode/msh-node-env",
  "optionalDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "husky": "4.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beecode-rs/msh-node-env.git"
  },
  "resolutions": {
    "minimist": "^1.2.3"
  },
  "scripts": {
    "audit": "npm audit --json | npx npm-audit-helper",
    "build": "npm run clean && tsc -p ./tsconfig-build.json",
    "build:test": "npm run clean && tsc -p ./tsconfig.json",
    "build:watch": "npm run clean && tsc-watch -p tsconfig-build.json --preserveWatchOutput",
    "coverage:unit": "nyc npm run test:unit",
    "clean": "rimraf ./lib/*",
    "eslint": "eslint . --ext .ts --quiet",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "preinstall": "npx npm-force-resolutions",
    "prettier": "prettier --check \"./src/**/*.{ts,json}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{ts,json}\"",
    "test": "npm run test:unit",
    "test:lint": "npm run test:unit -- --forbid-only --bail",
    "test:unit": "mocha --require ts-node/register ./src/**/*.test.ts",
    "test:unit:js": "mocha ./lib/**/*.test.js"
  },
  "typings": "./lib/index.d.ts",
  "version": "0.0.4"
}
